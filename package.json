{
  "name": "datproto-backup-n8n",
  "version": "1.0.0",
  "description": "Enterprise-grade N8N workflow backup and management system",
  "main": "scripts/index.js",
  "scripts": {
    "validate": "node scripts/validate-workflows.js",
    "lint": "eslint workflows/ scripts/ --ext .js,.json",
    "format": "prettier --write '**/*.{js,json,md}'",
    "backup": "node scripts/backup-workflows.js",
    "restore": "node scripts/restore-workflows.js",
    "health-check": "node scripts/health-check.js",
    "clean": "node scripts/clean-duplicates.js",
    "categorize": "node scripts/categorize-workflows.js",
    "setup": "node scripts/setup.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "dev": "nodemon scripts/index.js",
    "build": "echo 'No build step required for N8N workflows'",
    "start": "node scripts/index.js"
  },
  "keywords": [
    "n8n",
    "workflow",
    "backup",
    "automation",
    "enterprise",
    "ci-cd"
  ],
  "author": "Marco Ippel",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/marcoippel/datproto-backup-n8n.git"
  },
  "bugs": {
    "url": "https://github.com/marcoippel/datproto-backup-n8n/issues"
  },
  "homepage": "https://github.com/marcoippel/datproto-backup-n8n#readme",
  "devDependencies": {
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-json": "^3.1.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.5"
  },
  "dependencies": {
    "ajv": "^8.12.0",
    "chalk": "^4.1.2",
    "commander": "^12.0.0",
    "dotenv": "^16.4.1",
    "fs-extra": "^11.2.0",
    "glob": "^10.3.10",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "winston": "^3.11.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}